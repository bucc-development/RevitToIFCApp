{
    "version": "2.0",
    "logging": {
        "fileLoggingMode": "always",
        "logLevel": {
          "default": "Information",
          "Host": "Information",
          "Function": "Information",
          "Host.Aggregator": "Information"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "maxTelemetryItemsPerSecond" : 20,
                "excludedTypes" : "Dependency;Event;Request"
              }
        }
    }
}